{
  "name": "react-native-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint-staged": "LIST=`git diff-index --name-only --diff-filter=d HEAD | grep .*\\.js | grep -v json`; if [ \"$LIST\" ]; then eslint --no-eslintrc -c commit-eslint.json --fix $LIST; fi",
    "postinstall": "rndebugger-open",
